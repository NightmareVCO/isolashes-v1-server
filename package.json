{
  "name": "isolashes-server",
  "version": "0.0.1",
  "description": "Isolashes Server, an API restful server for the Isolashes project.",
  "author": "Vladimir Curiel",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start swc",
    "dev": "nest start swc --watch",
    "debug": "nest start --debug --watch",
    "prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "zenstack:generate": "pnpm zenstack generate && pnpm exec prisma generate && pnpm exec prisma migrate deploy && pnpm exec prisma db push",
    "zenstack:generate:dev": "pnpm zenstack generate && pnpm exec prisma generate && pnpm exec prisma migrate dev --name init && pnpm exec prisma db push",
    "zenstack:repl": "pnpm exec zenstack repl",
    "prisma:generate": "pnpm exec prisma generate && pnpm exec prisma migrate dev --name init && pnpm exec prisma db push",
    "prepare": "husky"
  },
  "dependencies": {
    "@fastify/helmet": "11.1.1",
    "@nestjs/common": "10.4.6",
    "@nestjs/config": "3.2.3",
    "@nestjs/core": "10.4.6",
    "@nestjs/mapped-types": "2.0.5",
    "@nestjs/platform-express": "10.4.6",
    "@nestjs/platform-fastify": "10.4.6",
    "@prisma/client": "5.21.1",
    "@zenstackhq/runtime": "2.7.4",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^3.6.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@compodoc/compodoc": "^1.1.24",
    "@nestjs/cli": "10.4.5",
    "@nestjs/schematics": "10.2.2",
    "@nestjs/testing": "10.4.6",
    "@swc/cli": "^0.3.14",
    "@swc/core": "^1.7.40",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.14",
    "@types/node": "20.17.1",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unicorn": "53.0.0",
    "husky": "^9.1.6",
    "jest": "29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "3.3.3",
    "prisma": "5.21.1",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.6.3",
    "zenstack": "^2.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}